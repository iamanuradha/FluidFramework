{
	"name": "@fluidframework/bundle-size-tools",
	"version": "0.10.0",
	"description": "Utility for analyzing bundle size regressions",
	"homepage": "https://fluidframework.com",
	"repository": {
		"type": "git",
		"url": "https://github.com/microsoft/FluidFramework.git",
		"directory": "build-tools/package/bundle-size-tools"
	},
	"license": "MIT",
	"author": "Microsoft and contributors",
	"main": "dist/index.js",
	"module": "lib/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"build": "pnpm run build:genver && concurrently npm:compile npm:lint && pnpm run build:docs",
		"build:copy": "copyfiles -u 1 \"src/**/*.fsl\" dist",
		"build:docs": "api-extractor run --local",
		"build:full": "pnpm run build",
		"build:full:compile": "pnpm run compile",
		"build:genver": "gen-version",
		"ci:build:docs": "api-extractor run",
		"clean": "rimraf dist *.tsbuildinfo",
		"compile": "concurrently npm:tsc npm:build:copy",
		"format": "pnpm run prettier:fix",
		"lint": "eslint --format stylish src",
		"lint:fix": "eslint --format stylish src --fix",
		"prettier": "prettier --check . --ignore-path ../../../.prettierignore",
		"prettier:fix": "prettier --write . --ignore-path ../../../.prettierignore",
		"test": "echo \"Error: no test specified\" && exit 1",
		"tsc": "tsc"
	},
	"dependencies": {
		"azure-devops-node-api": "^11.2.0",
		"jszip": "^3.10.1",
		"msgpack-lite": "^0.1.26",
		"pako": "^2.0.4",
		"typescript": "~4.5.5",
		"webpack": "^5.74.0"
	},
	"devDependencies": {
		"@fluidframework/build-common": "^1.1.0",
		"@fluidframework/eslint-config-fluid": "^2.0.0",
		"@microsoft/api-extractor": "^7.22.2",
		"@types/jszip": "^3.4.1",
		"@types/msgpack-lite": "^0.1.8",
		"@types/node": "^14.18.0",
		"@types/pako": "^2.0.0",
		"copyfiles": "^2.4.1",
		"eslint": "~8.6.0",
		"eslint-plugin-eslint-comments": "~3.2.0",
		"eslint-plugin-import": "~2.25.4",
		"eslint-plugin-unicorn": "~40.0.0",
		"eslint-plugin-unused-imports": "~2.0.0",
		"prettier": "~2.6.2",
		"rimraf": "^2.6.2"
	}
}
