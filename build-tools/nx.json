{
	"tasksRunnerOptions": {
		"default": {
			"runner": "@nrwl/nx-cloud",
			"options": {
				"parallel": 6,
				"cacheableOperations": ["build:genver", "build", "compile", "format", "lint"],
				"accessToken": "ZmY4MzQ3MDUtOWU1YS00MzJiLTg4ZTgtY2M3OWNlZjlhODk4fHJlYWQtd3JpdGU="
			}
		}
	},
	"namedInputs": {
		"default": ["{projectRoot}/**/*"],
		"test": ["{projectRoot}/package.json", "{projectRoot}/src/test/**/*"],
		"src": [
			"test",
			"{projectRoot}/tsconfig.*",
			"{projectRoot}/src/**/*",
			"!{projectRoot}/src/test/**/*"
		],
		"docs": ["src", "{projectRoot}/package.json", "{projectRoot}/api-extractor.json"]
	},
	"targetDefaults": {
		"build": {
			"inputs": ["src"],
			"dependsOn": ["^build"],
			"outputs": [
				"{projectRoot}/dist",
				"{projectRoot}/lib",
				"{projectRoot}/*.tsbuildinfo",
				"{projectRoot}/oclif.manifest.json",
				"{projectRoot}/README.md"
			]
		},
		"compile": {
			"inputs": ["src"],
			"dependsOn": ["^compile"]
		},
		"format": {
			"inputs": ["src"],
			"dependsOn": [],
			"outputs": ["{projectRoot}/src/**/*"]
		},
		"lint": {
			"inputs": ["src"],
			"dependsOn": ["^build"]
		},
		"test": {
			"inputs": ["test"],
			"dependsOn": ["^build"],
			"outputs": ["{projectRoot}/dist/test"]
		},
		"build:docs": {
			"dependsOn": ["^build:docs", "build"]
		},
		"build:manifest": {
			"dependsOn": ["clean:manifest", "build:readme"]
		},
		"build:readme": {
			"dependsOn": ["^build", "build:readme"]
		}
	}
}
